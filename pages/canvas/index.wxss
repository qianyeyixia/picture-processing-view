/* pages/canvas/index.wxss */
.header {
	margin-top: 40rpx;
	display: flex;
	justify-content: center;
	position: relative;
	width: 100vw;
	height:340px ;
}
.upload_img {
	width: 230px;
	height: 280px;
	display: flex;
	border-radius: 10rpx;
	align-items: center;
	justify-content: center;
	border: 1rpx solid #f333f3;
	position: absolute;
}


.swiper-content {
    width: 100%;
    height: 120rpx;
    overflow: hidden;
    white-space: nowrap;
    background-color: #ffffff;
    padding: 20rpx 0;
}

.frameBox {
    width: 120rpx;
    height: 80rpx;
    display: inline-block;
    margin-right: 20rpx;
}
.frameBox .frameBox-image {
    width: 100%;
    height: 100%;
}