/* pages/canvas/index.wxss */
.header {
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  position: relative;
  width: 100vw;
  height: 280px;
}
.body {
  margin-top: 40rpx;
}


.upload_img {
  width: 230px;
  height: 280px;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #f333f3;
  position: absolute;
  margin-bottom: 20rpx;
}

.swiper-content {
  width: 100%;
  height: 140rpx;
  overflow: hidden;
  white-space: nowrap;
  background-color: #ffffff;
  padding: 20rpx 0;
}

.frameBox {
  width: 140rpx;
  height: 80rpx;
  display: inline-block;
  margin-right: 20rpx;
}
.frameBox .frameBox-image {
  width: 100%;
  height: 100%;
}

.action-title {
  box-sizing: border-box;
  font-size: 18px;
  font-weight: 500;
  padding-left: 20px;
}
.action-title-tips {
  font-size: 14px;
  margin: 1em 0;
  font-weight: 600;
  color: red;
  padding-left: 20px;
}